

@model IndexView

    <h3><a href="add_user">Add a new user</a></h3>

 <table class="table table-hover">
    <thead>
        <tr>
            <th>User Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Created Date</th>
            <th>User Level</th>
            <th>Action</th>
           
        </tr>
    </thead>
      <tbody>
           @foreach(var user in Model.Users)
        {  
            <tr>
                <td>@user.user_id</td>
                <td><a asp-controller="Home" asp-action="ShowUserInfo" asp-route-id="@user.user_id">@user.first_name @user.last_name</a></td>
                <td>@user.email</td>
                <td>@user.created_at</td>
                <td>@user.user_level</td>
                <td><a href="">Edit</a> <a href="">Remove</a></td>
                    
            </tr>
        }
        
         
    </tbody>

    

</table>